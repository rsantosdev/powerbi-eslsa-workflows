{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "AuthorityUri": "https://login.microsoftonline.com/organizations/",
    "ClientId": "your-aad-client-(or application)-id",
    "TenantId": "your-azure-aad-tenantid",
    "Scope": [ "https://analysis.windows.net/powerbi/api/.default" ],
    "ClientSecret": "your-app-secret"
  },
  "PowerBI": {
    "WorkspaceId": "your-powerbi-workspace-id"
  },
  "Elsa": {
    "Server": {
      "BaseUrl": "http://localhost:5204"
    },
    "Smtp": {
      "Host": "localhost",
      "Port": "2525",
      "DefaultSender": "noreply@acme.com"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=PowerBiElsaScheduler;Integrated Security=True;"
  }
}
